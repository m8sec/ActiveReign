{
	"_heading": "GENERAL",
	"PWN3D_MSG"			: "Adm!n",
	"WORKSPACE"			: "default",


	"_heading": "SLACK CONFIG",
	"_comment": "https://api.slack.com/custom-integrations/legacy-tokens",
	"SLACK_API_TOKEN"	: "",
	"SLACK_CHANNEL"		: "",


	"_heading": "SHAREFINDER CONFIG",
	"_comment" : "Shares to skip during sharefinder/Access process (UPPER CASE)",
	"BLACKLIST_SHAREFINDER": ["IPC$", "PRINT$"],

	"_heading": "SPIDER CONFIG",
	"_comment": "Max seconds to retrieve a single file (default: 3)",
	"RETRIEVE_TIMEOUT": "3",

	"_comment": "Max seconds to parsing a single file (default: 3)",
	"PARSE_TIMEOUT": "3",

	"_comment": "Max file size to download and scan contents (bytes)",
	"MAX_FILE_SIZE": "50000",

	"_comment": "Max characters to parse per line of file",
	"MAX_CHAR": "1000",

	"_comment": "File Types to scan for data (LOWERCASE & WITHOUT STARTING .)",
	"WHITELIST_EXT": ["ps1", "bat", "vbs", "vb", "vba", "txt", "csv", "xlsx",
					  "docx", "pdf", "doc", "xls", "py", "rb", "pl", "c", "cs",
					  "xml", "ini", "json", "yaml", "rtf", "html", "log", "php",
					  "asp", "aspx", "ps1xml", "rdp", "kirbi", "kdb", "kdbx",
					  "pem", "crt", "ppk", "config", "conf"],

	"_comment" : "Shares to skip during spidering (UPPER CASE)",
	"BLACKLIST_SHARE": ["IPC$", "PRINT$" , "ADMIN$"],

	"_comment": "DIR's to skip during spider",
	"BLACKLIST_DIR": ["/Windows/", "/Program Files/", "/Program Files (x86)/", "/Windows/System32/"],

	"_comment": "Key words in filename to match (LOWER CASE)",
	"KEY_WORDS": ["password", "401k", "w9", "w2", "i9", "login"],

	"_comment": "File extensions to flag (LOWERCASE & WITHOUT LEADING .)",
	"KEY_EXT": ["rdp", "kdb", "kdbx", "pem", "crt", "ppk"],

	"_comment": "Key words to search for in header of xlsx documents(LOWER CASE)",
	"XLSX_KEY_HEADERS": ["password", "ssn", "emails"],

	"_comment": "Regex patters to look for in file contents",
	"REGEX": {"Password": "^.*[P|p]assword[:|=].*$",
			  "SSN": "^.*[0-9]{3}[-][0-9]{2}[-][0-9]{4}.*$",
			  "Credit Card" : ".*(3[47][0-9]{13}.*|.*(?:5[1-5][0-9]{2}|222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720)[0-9]{12}.*|.*4[0-9]{12}(?:[0-9]{3})).*"}
}